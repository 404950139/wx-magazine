.nav {
    position: fixed;
    top:0;
    left:0;
    z-index:9999;
}
.to-catalog {
    display:inline-block;
    border:1px solid #000;
    padding:10rpx 20rpx;
    font-size:28rpx;
}
.recommend {
    margin-top:110rpx;
    display:block;
}
.loading-container {
    position:fixed;
    left:0;
    top:0;
    width:100%;
    height:100%;
    background-color:#fff;
}
.loading {
    position:absolute;
    top:50%;
    left:50%;
    margin-top:-30rpx;
    margin-left:-30rpx;
}